{
  "name": "stack-overflow-api-clone",
  "version": "1.0.0",
  "description": "Built with NodeJS and MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel server -d dist",
    "dev": "env-cmd -f ./.env nodemon --exec babel-node src/index.js",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toluola/stack-overflow-api-clone.git"
  },
  "author": "tolu' olaniyan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toluola/stack-overflow-api-clone/issues"
  },
  "homepage": "https://github.com/toluola/stack-overflow-api-clone#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "engine.io": "^3.4.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.5.4",
    "mongoose": "^5.9.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "env-cmd": "^10.1.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
